<% include ../partials/header %>
<% include ../partials/nav %>

<div class="container">
    <h1 class="mb-4">All users</h1>
    <%users.forEach(user => { %>
    <div class="card mt-4">
        <div class="card-body">
            <h4 class="card-title">
                Name: <%= user.name %>
            </h4>
            <h4 class="card-title">
                Email: <%= user.email %>
            </h4>
            <h4 class="card-title">
                Password: <%= user.password %>
            </h4>
            <div class="card-subtitle text-muted mb-2">
                <%= user.createAt.toLocaleDateString() %>
            </div>
        </div>
    </div>
    <% }) %>

</div>






<% include ../partials/footer %>